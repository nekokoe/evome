<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
             xmlns:g="urn:import:com.google.gwt.user.client.ui">
    <ui:style>

    </ui:style>
    <g:Grid>
        <g:row>
            <g:customCell>
                <g:Label>Project ID</g:Label>
            </g:customCell>
            <g:customCell>
                <g:Label ui:field="lblID">test</g:Label>
            </g:customCell>                    
        </g:row>
        <g:row>
            <g:customCell>
                <g:Label>Project Name</g:Label>
            </g:customCell>
            <g:customCell>
                <g:Label ui:field="lblName">test</g:Label>
            </g:customCell>                    
        </g:row>
        <g:row>
            <g:customCell>
                <g:Label>Project Owner</g:Label>
            </g:customCell>
            <g:customCell>
                <g:Label ui:field="lblOwner">test</g:Label>
            </g:customCell>                    
        </g:row>
        <g:row>
            <g:customCell>
                <g:Label>Created on</g:Label>
            </g:customCell>
            <g:customCell>
                <g:Label ui:field="lblCreate">test</g:Label>
            </g:customCell>                    
        </g:row>
        <g:row>
            <g:customCell>
                <g:Label>Last Modified on</g:Label>
            </g:customCell>
            <g:customCell>
                <g:Label ui:field="lblModify">test</g:Label>
            </g:customCell>                    
        </g:row>        
        <g:row>
            <g:customCell>
                <g:Label>Project Details</g:Label>
            </g:customCell>
            <g:customCell>
                <g:Label ui:field="lblComment">test</g:Label>
            </g:customCell>                    
        </g:row>                                
    </g:Grid>
</ui:UiBinder>